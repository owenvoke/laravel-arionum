{
    "name": "pxgamer/laravel-arionum",
    "type": "library",
    "description": "A Laravel adapter for the Arionum PHP wrapper.",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "pxgamer/arionum-php": "^3.0",
        "illuminate/support": "^5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "pxgamer\\LaravelArionum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pxgamer\\LaravelArionum\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "pxgamer\\LaravelArionum\\ArionumServiceProvider"
            ],
            "aliases": {
                "Arionum": "pxgamer\\LaravelArionum\\ArionumFacade"
            }
        }
    }
}
