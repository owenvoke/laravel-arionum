{
    "name": "owenvoke/laravel-arionum",
    "type": "library",
    "description": "A Laravel adapter for the Arionum PHP wrapper.",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "facade/ignition-contracts": "^1.0",
        "illuminate/support": "^6.0|^7.0",
        "owenvoke/arionum-php": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "orchestra/testbench": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "OwenVoke\\LaravelArionum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OwenVoke\\LaravelArionum\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "OwenVoke\\LaravelArionum\\ArionumServiceProvider"
            ],
            "aliases": {
                "Arionum": "OwenVoke\\LaravelArionum\\ArionumFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
